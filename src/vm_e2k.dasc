        | ldwsm     5, RB, TAB->asize, T2
        | --
        | lddsm     3, RB, TAB->array, T3
        | cmpesb    4, ITYPE, LJ_TSTR, pred3
        | lddsm     5, RB, TAB->metatable, TMP1
        | --
        | pass      pred0, p0
        | pass      pred2, p1
        | pass      pred3, p2
        | landp     p0, p1, p4
