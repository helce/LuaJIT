    | // RB = L, L->base = new base, L->top = top 
    | ldd       3, RB, L->base, BASE
    | ldd       5, RB, L->top, RD
    | wait_load BASE, 0
    | --
    | ldd       3, BASE, -16, RB
    | subd      4, RD, BASE, RD
    | wait_load RB, 0
    | --
    | getfd     3, RB, (47 << 6), RB
    | addd      4, RD, 0x8, RD
    | --
