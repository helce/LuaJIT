        | ldd       5, BASE, RC_E, RC
        | disp      ctpr1, ->vmeta_tsetv
        | --
        | pipe_scale 0, 1, 2, 3
        | pipe_fetch 5
        | disp      ctpr2, ->BC_TSETS_Z
        | --
        | pipe_extract 0, 1, 2, 3, 4
        | wait_load RB, 2
        | --
        | sard      3, RB, 0x2f, T1
        | fdtoistr  4, RC, TMP0
        | getfd     5, RB, (47 << 6), RB
        | wait      TMP0, 0, 4
        | --
        | cmpesb    3, T1, LJ_TTAB, pred0
        | istofd    4, TMP0, TMP1
        | sard      5, RC, 0x2f, ITYPE
        | wait      TMP1, 0, 4
        | --
        | fcmpeqdb  3, RC, TMP1, pred2      // Integer key?  Convert number to int and back and compare.
