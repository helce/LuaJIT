        | ldw       5, RC, STR->sid, TMP1
        | disp      ctpr2, >1
        | --
        | shld      4, ITYPE, 0x2f, ITYPE
        | ldd       5, RB, TAB->node, T2
        | disp      ctpr1, >2
        | wait_load TMP0, 1
        | --
        | andd      3, TMP0, TMP1, TMP1     // idx = str->sid & tab->hmask
        | ord       4, ITYPE, RC, ITYPE
        | addd      2, 0x0, 0x0, T1
