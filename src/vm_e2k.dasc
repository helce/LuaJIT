        | ct        ctpr1, ~pred0           // vmeta_tsetv
        | --
        | ldw       3, RC, 0x0, TMP0
        | lddsm     5, BASE, RA_E, T0
        | wait_load TMP0, 0
        | --
        | cmpesb    4, TMP0, LJ_TNIL, pred0 // Previous value is nil?
        | --
        | pass      pred0, p0
        | pass      pred1, p1
        | pass      pred2, p2
        | landp     p0, ~p1, p4
        | landp     p4, p2, p5
        | pass      p5, pred0
        | wait_pred_ct pred0, 0
