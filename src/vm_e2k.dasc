    | ldd 3, RA, -8, PC, pred1              // Move initial function up.
    | ct ctpr1, pred0
    | nop 2
    | --
    | addd 4, RA, 0x8, RA, pred1
    | std 5, RA, 0x0, PC, pred1
    | --
