    | --
    | addd      0, 0x0, LUA_MINSTACK, CARG2
    | addd      1, RB, 0x0, CARG1
    | --
    | call      ctpr1, wbs = 0x8            // lj_state_growstack(lua_State *L, int n)
    | --
    | ldd       3, RB, L->base, BASE
    | ldd       5, RB, L->top, RD
    | wait_load BASE, 0
    | --
    | ldd       3, BASE, -16, RB
    | subd      4, RD, BASE, RD
    | wait_load RB, 0
    | --
    | getfd     3, RB, (47 << 6), RB
    | addd      4, RD, 0x8, RD
