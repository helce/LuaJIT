    |.macro coroutine_resume_wrap, resume
    |.if resume
    |->ff_coroutine_resume:
    | ldd 3, BASE, -8, PC
    | ldd 5, BASE, 0x0, RB
    | disp ctpr1, ->fff_fallback
    | nop 2
    | --
    | sard 3, RB, 0x2f, ITYPE
    | getfd 4, RB, (47 << 6), RB
    | --
    | cmpbdb 3, RD, (1+1)*8, pred0
    | cmpesb 4, ITYPE, LJ_TTHREAD, pred1
    | nop 1
    | --
    | pass pred0, p0
    | pass pred1, p1
    | landp ~p0, p1, p4
    | pass p4, pred0
    | --
    | std 2, STACK, SAVE_PC, PC, pred0
    | ct ctpr1, ~pred0
    | --
    |.else
    |->ff_coroutine_wrap_aux:
    | ldd 3, BASE, -16, RB
    | ldd 5, BASE, -8, PC
    | nop 2
    | --
    | getfd 3, RB, (47 << 6), RB
    | --
    | ldd 3, RB, CFUNC->upvalue[0].gcr, RB
    | nop 2
    | --
    | std 2, STACK, SAVE_PC, PC
    | getfd 3, RB, (47 << 6), RB
    | --
    |.endif
